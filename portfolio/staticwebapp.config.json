{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/*.{png,jpg,jpeg,gif,svg,css,js,ico}"]
  },
  "routes": [
    {
      "route": "/*",
      "redirect": "https://umeshdas.com{path}",
      "statusCode": 308
    }
  ],
  "globalHeaders": {
    "Strict-Transport-Security": "max-age=31536000; includeSubDomains; preload"
  }
}